{
    "name": "better-toml",
    "displayName": "Better TOML",
    "description": "Better TOML Language support",
    "version": "0.3.1",
    "publisher": "bungcip",
    "license": "MIT",
    "homepage": "https://github.com/bungcip/better-toml/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/bungcip/better-toml.git"
    },
    "bugs": {
        "url": "https://github.com/bungcip/better-toml/issues",
        "email": "bungcip@gmail.com"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "keywords": [
        "toml"
    ],
    "activationEvents": [
        "onLanguage:toml"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "languages": [
            {
                "id": "toml",
                "aliases": [
                    "TOML",
                    "toml"
                ],
                "extensions": [
                    ".toml"
                ],
                "mimetypes": [
                    "text/x-toml"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "toml",
                "scopeName": "source.toml",
                "path": "./syntaxes/TOML.tmLanguage"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "dependencies": {
        "vscode-languageclient": "^2.6.0",
        "vscode-languageserver": "^2.6.0",
        "toml": "^2.3.3"
    },
    "devDependencies": {
        "typescript": "2.5.2",
        "vscode": "^1.0.3",
        "mocha": "^3.1.2",
        "@types/node": "^6.0.46",
        "@types/mocha": "^2.2.32"
    }
}
